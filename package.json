  "description": "A generative artwork",
  "main": "index.js",
  "scripts": {
    "install": "jspm install",
    "start": "browser-sync start --startPath \"index.html\" --files \"src\" --server --no-notify",
    "build": "jspm bundle-sfx main.js build/main.min.js && jspm bundle-sfx main-receiver.js build/main-receiver.min.js",
